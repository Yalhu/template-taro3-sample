{
  "name": "taro-app",
  "version": "2.0.1",
  "private": true,
  "description": "taro3跨端开发",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:h5-beta": "cross-env BUILD_ENV=beta taro build --type h5",
    "build:h5-mstatic": "cross-env BUILD_ENV=mstatic taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:weapp-wxabuild": "cross-env BUILD_ENV=wxabuild taro build --type weapp --blended --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prod:weapp": "taro build --type weapp --blended",
    "prod:weapp-wxabuild": "cross-env BUILD_ENV=wxabuild taro build --type weapp --blended",
    "prod:weapp-wxapp": "cross-env BUILD_ENV=wxapp taro build --type weapp --blended",
    "prod:comp": "taro build native-components --type weapp",
    "debug": "node --inspect node_modules/@tarojs/cli/bin/taro build --watch --type weapp",
    "test": "cross-env JEST_REPORT_FILE='./test/report/test-report.json' TARO_ENV=h5 jest --config test/jest.conf.js",
    "fix": "eslint --fix --ext .ts --ext .tsx src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "sideEffects": [
    "plugins/track/runtime.ts",
    "*.css",
    "*.less",
    "*.scss"
  ],
  "author": "",
  "dependencies": {
    "@babel/core": "7.20.12",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.20.7",
    "@babel/runtime": "7.20.7",
    "@swc/core": "1.3.6",
    "@tarojs/cli": "3.5.8",
    "@tarojs/components": "3.5.8",
    "@tarojs/helper": "3.5.8",
    "@tarojs/plugin-framework-react": "3.5.8",
    "@tarojs/plugin-indie": "0.0.7",
    "@tarojs/plugin-platform-weapp": "3.5.8",
    "@tarojs/react": "3.5.8",
    "@tarojs/router": "3.5.8",
    "@tarojs/runtime": "3.5.8",
    "@tarojs/service": "3.5.8",
    "@tarojs/shared": "3.5.8",
    "@tarojs/taro": "3.5.8",
    "@tarojs/taro-h5": "3.5.8",
    "@tarojs/taro-loader": "3.5.8",
    "@tarojs/webpack5-prebundle": "3.5.8",
    "@tarojs/webpack5-runner": "3.5.8",
    "babel-loader": "8.2.5",
    "babel-preset-taro": "3.5.8",
    "buffer": "6.0.3",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "fs-extra": "9.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest-bamboo-reporter": "1.3.0",
    "jest-html-reporter": "3.7.0",
    "jsencrypt": "3.2.1",
    "lint-staged": "10.5.4",
    "lodash-es": "4.17.21",
    "md5": "2.3.0",
    "numeral": "2.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "0.14.0",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.24",
    "@types/lodash-es": "4.17.6",
    "@types/numeral": "2.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.1",
    "babel-jest": "26.6.3",
    "eslint": "8.31.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.2.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-taro": "3.5.8",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sonarjs": "0.15.0",
    "eslint-webpack-plugin": "2.7.0",
    "fork-ts-checker-webpack-plugin": "7.2.14",
    "html-loader": "^4.2.0",
    "html-webpack-asset-tag-alter-plugin": "^1.0.0",
    "husky": "8.0.3",
    "jest": "26.6.3",
    "stylelint": "9.3.0",
    "typescript": "4.9.4",
    "webpack-bundle-analyzer": "4.7.0",
    "webpack-ssi-include-loader": "^1.6.1"
  }
}
